<%# default partial to display solr document fields in catalog index view -%>
<dl class="document-metadata dl-invert row">  
  <% index_fields(document).each do |field_name, field| -%>
    <% if presenter.render_field? field %>
	    <dt class="blacklight-<%= field_name.parameterize %> col-md-3"><%= presenter.field_label field_name %></dt>
	    <dd class="blacklight-<%= field_name.parameterize %> col-md-9"><%= presenter.field_value field_name %></dd>
    <% end -%>
  <% end -%>
</dl>
