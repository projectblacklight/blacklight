<%# default partial to display solr document fields in catalog index view -%>
<dl class="defList">
  
  <% index_field_names.each do |solr_fname| -%>
    <% if document.get solr_fname %>
	<dt class="blacklight-<%= solr_fname.parameterize %>"><%= h index_field_labels[solr_fname]%></dt>
	<dd class="blacklight-<%= solr_fname.parameterize %>"><%= h document.get(solr_fname) %></dd>
    <% end -%>
  <% end -%>

</dl>
